#include "/Engine/Public/Platform.ush"

// Input value
float InputValue;
float Multiplier;

// Output buffer (single element)
RWBuffer<float> OutputValue;

[numthreads(1, 1, 1)]
void MainCS(uint3 ThreadId : SV_DispatchThreadID)
{
    // Simple operation: multiply input by multiplier and store
    OutputValue[0] = InputValue * Multiplier;
}
